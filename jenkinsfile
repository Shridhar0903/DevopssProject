pipeline {
    agent any
    stages {
        stage('Clone Repo') {
            steps {
                git 'https://github.com/Shridhar0903/DevopssProject.git'
            }
        }
        stage('Build Docker Image') {
            steps {
                script {
                    dockerImage = docker.build("mydevopsproject")
                }
            }
        }
        stage('Run Container') {
            steps {
                script {
                    dockerImage.run("-d -p 8081:80")
                }
            }
        }
    }}
